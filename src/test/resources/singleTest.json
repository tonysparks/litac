{
    description: "Single Test",
    disabled: false,
    program: '''                
        @include("assert.h");
        @foreign func assert(e:bool):void;
        
        %definitions%
        
        func main(len:i32, args:char**):i32 {
            %test%
        }
    '''
    tests: [                           
        
        { 
            name: "Method definition order",
            definitions: '''                                
                struct Test {
                    x: i32
                }
                
                
                func (t: Test*) a() : i32 {
                    return t.b()
                }
                
                func (t: Test*) b() : i32 {
                    return t.x
                }
            ''',                       
            code: '''         
                var t = Test{4}                            
                assert(t.a() == 4)
            ''',                      
        },
    ]
}