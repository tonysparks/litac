{
    description: "Single Test",
    disabled: false,
    debug: true,
    program: '''                
        @include("assert.h");
        @foreign func assert(e:bool):void;
        
        %definitions%
        
        func main(len:i32, args:char**):i32 {
            %test%
        }
    '''
    tests: [                           
        { 
            name: "Address of Array Element",
            definitions: '''
                const array = [3]i32 {
                    1, 2, 3
                }
            ''',                       
            code: '''         
                var a = &array[0]
                assert(a != 1)
            ''',                      
        },
    ]
}