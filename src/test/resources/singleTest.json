{
    description: "Single Test",
    program: '''                
        @include("assert.h");
        @foreign func assert(e:bool):void;
        
        %definitions%
        
        func main(len:i32, args:char**):i32 {
            %test%
        }
    '''
    tests: [                
        {
            name: "Implicit cast binary expr",                       
            code: '''
                var x : i32 = 10
                var a : f32 = x / 2.0f
                
                assert(a == 5.0f)
            ''',
        },     
    ]
}